<section id="cheese_form">
  <h2>Update Cheese Form</h2>

  <form method="POST" action="/update_form_request/:id">
    <label for="name">Name: </label>
    <input type="text" placeholder=<%="#{@cheese["name"]}"%> name="name" id="name" />
    <br/><br/>
    <label  for="country">Country: </label>
    <input type="text" placeholder=<%="#{@cheese["country"]}"%> name="country" id="country" />
    <br/><br/>
    <label for="milk_type">Milk Type: </label>
    <input type="text" placeholder=<%="#{@cheese["milk_type"]}"%> name="milk_type" id="milk_type" />
    <br/><br/>
    <label for="stank_level">Stank Level(1-5): </label>
    <input type="text" placeholder=<%="#{@cheese["stank_level"]}"%> name="stank_level" id="stank_level" />
    <br/><br/>
    <label for="desc">Description: </label>
    <input type="text" placeholder=<%="#{@cheese["desc"]}"%> name="desc" id="desc" />
    <br/><br/>
    <label for="image_url">Image URL: </label>
    <input type="text" placeholder=<%="#{@cheese["image_url"]}"%>name="image_url" id="image_url" />
    <br/><br/>
    <input type="submit" value="Submit" />
  </form>

  <% if @show_contact_success_message %>
    <p class="message">Your new cheese has been added!</p>
  <% end %>

</section>


